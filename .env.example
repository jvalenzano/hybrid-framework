# Environment Variables Configuration
# Copy this file to .env and fill in your actual values

# Agno Configuration
AGNO_API_KEY=your_agno_api_key_here
AGNO_WORKSPACE_ID=your_workspace_id_here
AGNO_ENV=development  # Options: development, staging, production

# Agent OS (Builder Methods) Configuration
AGENT_OS_API_KEY=your_agent_os_api_key_here
AGENT_OS_PROJECT_ID=your_project_id_here
AGENT_OS_ENDPOINT=https://api.buildermethods.com/v1

# Optional: Monitoring & Analytics
PROMETHEUS_ENDPOINT=http://localhost:9090
GRAFANA_API_KEY=optional_grafana_key
JAEGER_ENDPOINT=http://localhost:14268

# Development Settings
DEBUG=false
LOG_LEVEL=INFO
CACHE_TTL=300
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
